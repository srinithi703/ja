import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        if (!sc.hasNextLine()) return;
        String s = sc.nextLine();
        String result = s.replaceAll("[AEIOUaeiou]", "");
        System.out.print(result);
        sc.close();
    }
}
