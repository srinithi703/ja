import java.util.Scanner;

public class AlicePathFinder {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        
        int input = Integer.parseInt(scanner.nextLine());

        
        System.out.println("Bird said:" + input);

       
        int path;
        if (input <= 50) {
            path = input % 6;
        } else {
            path = input % 21;
        }

        
        System.out.println("Alice must go in path-" + path);

        scanner.close();
    }
}
